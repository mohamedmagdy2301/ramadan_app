		<key>NSPushNotificationsUsageDescription</key>
		<string>We use notifications to keep you updated.</string>
		<!-- Vibration Permission -->
		<key>NSMotionUsageDescription</key>
		<string>Allowing vibration for notifications.</string>
		<!-- Alarm and Notification Policy -->
		<key>NSCalendarsUsageDescription</key>
		<string>Allows scheduling exact alarms and reminders.</string>
		<!-- Internet Access -->
		<key>NSAppTransportSecurity</key>
		<dict>
			<key>NSAllowsArbitraryLoads</key>
			<true/>
		</dict>
		<key>UIStatusBarHidden</key>
		<false/>
		<key>UIViewControllerBasedStatusBarAppearance</key>
		<false/>
